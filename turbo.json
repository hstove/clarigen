{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "installer/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": [],
      "parallel": true
    },
    "dev": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "publish:dev": {
      "outputs": []
    }
  },
  "globalEnv": ["NODE_ENV", "NEXT_PUBLIC_STACKS_API_URL", "CI"]
}
