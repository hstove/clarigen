{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "installer/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "type-stubs": {
      "outputs": []
    }
  },
  "globalEnv": ["NODE_ENV", "NEXT_PUBLIC_STACKS_API_URL", "CI", "LOG_TX_CALLS"]
}
