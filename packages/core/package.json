{
  "version": "4.0.2-alpha.0",
  "license": "MIT",
  "types": "./dist/index.d.cts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "import": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdown --watch",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier --check \"./**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit -p tsconfig-test.json",
    "publish:dev": "pnpm build && yalc publish --push",
    "copy-types": "node copy-types.js"
  },
  "prettier": "@stacks/prettier-config",
  "name": "@clarigen/core",
  "author": "Hank Stoever",
  "dependencies": {
    "@scure/base": "^1.1.6",
    "@stacks/blockchain-api-client": "7.10.0",
    "@stacks/common": "^7.0.2",
    "@stacks/network": "^7.0.2",
    "@stacks/stacks-blockchain-api-types": "7.10.0",
    "@stacks/transactions": "7.1.0"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./deployment": {
      "require": "./dist/deployment.cjs",
      "import": "./dist/deployment.mjs"
    },
    "./package.json": "./package.json"
  }
}
