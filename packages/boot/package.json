{
  "version": "4.1.0",
  "license": "MIT",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./mainnet": {
      "require": "./dist/mainnet.cjs",
      "import": "./dist/mainnet.mjs"
    },
    "./testnet": {
      "require": "./dist/testnet.cjs",
      "import": "./dist/testnet.mjs"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "clarigen": "./dist/run-cli.js"
  },
  "scripts": {
    "start": "tsx src/run-cli.ts",
    "dev": "tsdown --watch",
    "build": "shx rm -rf dist && tsdown",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier --check \"./**/*.{ts,js}\"",
    "format:fix": "prettier -w \"./**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit -p tsconfig-test.json",
    "type-stubs": "clarigen",
    "publish": "bun publish"
  },
  "prettier": "@stacks/prettier-config",
  "name": "@clarigen/boot",
  "author": "Hank Stoever",
  "devDependencies": {
    "tsx": "^4.7.2"
  },
  "dependencies": {
    "@clarigen/core": "workspace:*",
    "@clarigen/cli": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs"
}
