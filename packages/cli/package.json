{
  "version": "2.0.6",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "clarigen": "./dist/run-cli.js"
  },
  "scripts": {
    "start": "tsx src/run-cli.ts",
    "dev": "tsup --watch",
    "build": "pnpm type-stubs && shx rm -rf dist && tsup",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier --check \"./**/*.{ts,js}\"",
    "format:fix": "prettier -w \"./**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit -p tsconfig-test.json",
    "type-stubs": "tsx scripts/generate-type-stubs.ts && prettier --loglevel error -w ./src/**/*.ts"
  },
  "prettier": "@stacks/prettier-config",
  "name": "@clarigen/cli",
  "author": "Hank Stoever",
  "devDependencies": {
    "tsx": "^4.7.2",
    "vitest": "1.5.0"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@clarigen/core": "workspace:*",
    "@hirosystems/clarinet-sdk": "2.6.0",
    "@hirosystems/clarinet-sdk-wasm": "2.6.0",
    "@iarna/toml": "^2.2.5",
    "@stacks/transactions": "6.13.1",
    "chokidar": "^3.6.0",
    "clipanion": "4.0.0-rc.3",
    "execa": "^8.0.1",
    "ora": "^8.0.1",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0",
    "smol-toml": "^1.1.4",
    "yaml": "^2.4.1",
    "zod": "^3.22.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
