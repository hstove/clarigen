{
  "version": "4.0.0",
  "license": "MIT",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "clarigen": "./dist/run-cli.js"
  },
  "scripts": {
    "start": "tsx src/run-cli.ts",
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && prettier --check \"./**/*.{ts,js}\"",
    "format:fix": "prettier -w \"./**/*.{ts,js}\"",
    "typecheck": "tsc --noEmit -p tsconfig-test.json",
    "type-stubs": "tsx scripts/generate-type-stubs.ts && prettier --loglevel error -w ./src/**/*.ts"
  },
  "prettier": "@stacks/prettier-config",
  "name": "@clarigen/cli",
  "author": "Hank Stoever",
  "devDependencies": {
    "@stacks/clarinet-sdk": "3.9.2",
    "@stacks/clarinet-sdk-wasm": "3.9.2",
    "tsx": "^4.7.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "@stacks/clarinet-sdk": "^3.9.2",
    "@stacks/clarinet-sdk-wasm": "^3.9.2"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@clarigen/core": "workspace:*",
    "@iarna/toml": "^2.2.5",
    "@stacks/transactions": "7.1.0",
    "arktype": "^2.1.25",
    "chokidar": "^3.6.0",
    "clipanion": "4.0.0-rc.3",
    "execa": "^8.0.1",
    "ora": "^8.0.1",
    "pino": "^8.20.0",
    "pino-pretty": "^11.0.0",
    "smol-toml": "^1.1.4",
    "yaml": "^2.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
